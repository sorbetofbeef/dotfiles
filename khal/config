[calendars]
[[calendars]]
path = ~/workspace/calendars/*
type = discover
color = dark red
[[work]]
path = ~/workspace/calendars/work

[[home]]
path = ~/workspace/calendars/home

[locale]
local_timezone=America/Los_Angeles
default_timezone=America/Los_Angeles
timeformat = %H:%M
dateformat = %m/%d/%Y
longdateformat = %m/%d/%Y
datetimeformat = %m/%d/%Y %H:%M
longdatetimeformat = %m/%d/%Y %H:%M

[default]
default_calendar = work
